{
  "expo": {
    "name": "outscore",
    "slug": "outscore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "outscore",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imeggcellent.outscore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#187C56"
      },
      "package": "com.imeggcellent.outscore"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#187C56"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SourceSans3-Regular.ttf",
            "./assets/fonts/SourceSans3-SemiBold.ttf",
            "./assets/fonts/SourceSans3-Bold.ttf"
          ]
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "assetBundlePatterns": [
      "assets/**"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7259011d-cbba-4495-ad04-23459bcc31b0"
      }
    }
  }
}

